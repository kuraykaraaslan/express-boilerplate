{
  "name": "express-boilerplate",
  "version": "1.0.6",
  "description": "Express based API boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only server.ts",
    "build": "tsc",
    "db:migrate": "prisma migrate dev --name init",
    "db:generate": "prisma generate",
    "db:seed": "ts-node prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "vercel-build": "prisma generate && prisma migrate deploy && npm run build",
    "test": "jest --detectOpenHandles --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuraykaraaslan/express-boilerplate.git"
  },
  "author": "Kuray Karaaslan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kuraykaraaslan/express-boilerplate/issues"
  },
  "homepage": "https://github.com/kuraykaraaslan/express-boilerplate#readme",
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/csurf": "^1.11.5",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.1",
    "@types/geoip-lite": "^1.4.4",
    "@types/google-libphonenumber": "^7.4.30",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.3",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.3",
    "dotenv": "^16.5.0",
    "eslint": "^9.25.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "express": "^5.1.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "3.5.3",
    "prisma": "^6.7.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
